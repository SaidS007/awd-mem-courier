{
    "name": "memcourrier",
    "version": "2505.0.2",
    "description": "MEM Courrier (Version dérivée de Maarch Courrier)",
    "engines": {
        "npm": ">=8.19.3",
        "node": ">=18.13.0"
    },
    "scripts": {
        "ng": "ng",
        "ng-update": "ng update --allow-dirty --force",
        "build-prod": "ng build --project \"memCourrier\" --configuration production",
        "build-watch": "ng build --project \"memCourrier\" --watch",
        "build": "ng build --project \"memCourrier\"",
        "build-release": "npm run build-prod && npm run",
        "check-update": "npm outdated",
        "dep-list": "npm list --depth=0",
        "upgrade": "npm upgrade --save",
        "lint": "eslint -c .eslintrc --ext .ts,.html --format gitlab src/frontend/",
        "lint-fix": "eslint -c .eslintrc --ext .ts src/frontend/ --fix",
        "reload-packages": "rm -Rf node_modules && npm cache clean --force && npm config set legacy-peer-deps true && npm ci",
        "reload-packages-prod": "rm -Rf node_modules && npm cache clean --force && npm config set legacy-peer-deps true && npm ci --production",
        "create-component": "echo Component name ? && read varname && ng g c $varname --module app --style=scss --project=memCourrier",
        "create-component-admin": "echo Component name ? && read varname && ng g c administration/$varname --module administration --style=scss --project=memCourrier",
        "karma:test-with-coverage": "ng test --project=memCourrier --no-watch --no-progress --browsers=ChromeHeadless --code-coverage",
        "karma:test-ci": "ng test --project=memCourrier --karma-config karma.conf-ci.js --no-watch --no-progress --browsers=ChromeHeadlessCI --code-coverage",
        "karma:test": "ng test --project=memCourrier"
    },
    "keywords": [],
    "author": "Maarch",
    "license": "GPL-3.0",
