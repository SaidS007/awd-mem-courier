<?xml version="1.0" encoding="utf-8"?>
<ROOT>
    <EXTRA>
        <blueWay>true</blueWay> <!-- Si activé, modification des routes pour utiliser celles de BlueWay -->
    </EXTRA>
    <SETTINGS>
        <apiUrl></apiUrl>
        <apiKey></apiKey>
        <pageSize>100</pageSize>
        <memSuperUserId></memSuperUserId>
        <minDepotInMinutes>60</minDepotInMinutes>
        <externalIdValue>eCitiz</externalIdValue>
        <etatDemande>SAISIE_TERMINEE</etatDemande>
        <getDemandeApi>/ws/gn/api/v1/demandes</getDemandeApi>
        <sendEmailApi>/ws/gn/api/v1/demande/mail</sendEmailApi>
        <sendNotesApi>/ws/gn/api/v1/demande/commentaire</sendNotesApi>
        <updateStatusApi>/ws/gn/api/v1/demande/status</updateStatusApi>
        <getDemandePiecesApi>/ws/gn/api/v1/demandesPieces</getDemandePiecesApi>
    </SETTINGS>
    <MEM_DATA>
        <status>NEW</status>
        <model_id>1</model_id>
        <default_destination>3</default_destination> <!-- Si le sous-thème ne corresponds pas à la table de correspondance destination_correspondances, on utilise cette destination par défaut -->
        <default_initiator>3</default_initiator> <!-- Si le sous-thème ne corresponds pas à la table de correspondance destination_correspondances, on utilise ce service initiateur par défaut -->
        <priority>poiuytre1357nbvc</priority>
        <autres_infos_custom_id>3</autres_infos_custom_id> <!-- Identifiant du champs personnalisé pour contenir les autres informations relatives à la demande (STRING) -->
        <domaine_custom_id>4</domaine_custom_id> <!-- Identifiant du champs personnalisé pour contenir le libelleDomaine (STRING) -->
        <theme_custom_id>5</theme_custom_id> <!-- Identifiant du champs personnalisé pour contenir le libelleTheme (STRING) -->
        <sous_theme_custom_id>8</sous_theme_custom_id> <!-- Identifiant du champs personnalisé pour contenir le codeSousTheme. Ne pas afficher dans formulaire, champ technique (STRING) -->
        <label_sous_theme_custom_id>7</label_sous_theme_custom_id> <!-- Identifiant du champs personnalisé pour contenir le libelleSousTheme (STRING) -->
        <pj_justificative_type>simple_attachment</pj_justificative_type> <!-- Type de pièces jointes MEM pour les pièces justificatives arrivant d'e-Citiz -->
        <pj_instructions_type>instructions</pj_instructions_type> <!-- Type de pièces jointes MEM pour les pièces d'instructions arrivant d'e-Citiz -->
        <pj_response_non_engageante>non_engageante</pj_response_non_engageante> <!-- Type de pièces jointes MEM pour envoi des PJ de réponses non engageante à e-Citiz -->
        <nature_custom_id>1</nature_custom_id> <!-- Identifiant du champs personnalisé pour contenir la nature. Table de correspondance associée également (STRING) -->
        <type_usager_custom_id>26</type_usager_custom_id> <!-- Identifiant du champs personnalisé pour contenir le type de l'usager -->
        <destination_correspondances>
            <ENTRY>
                <ECITIZ>PQ_PQ_GUE</ECITIZ>
                <MEM_DESTINATION>1</MEM_DESTINATION>
                <MEM_INITIATOR>2</MEM_INITIATOR>
            </ENTRY>
            <ENTRY>
                <ECITIZ>PQ_PQ_GRH</ECITIZ>
                <MEM_DESTINATION>3</MEM_DESTINATION>
                <MEM_INITIATOR>4</MEM_INITIATOR>
            </ENTRY>
            <ENTRY>
                <ECITIZ>PQ_PQ_INTRS</ECITIZ>
